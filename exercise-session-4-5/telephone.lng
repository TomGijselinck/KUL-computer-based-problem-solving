! Max flow template;

MODEL:
SETS:
  NODES/1..6/;
  ARCS(NODES,NODES)/1,2 1,3 2,4 2,5 3,4 3,5 4,6 5,6 6,1/:CAP,FLOW;
ENDSETS
MAX=FLOW(4,6) + FLOW(5,6);
@FOR(ARCS(I,J):FLOW(I,J)<CAP(I,J));
@FOR(NODES(I):
  @SUM(ARCS(J,I):FLOW(J,I)) = @SUM(ARCS(I,J):FLOW(I,J)));
DATA:
  CAP=500 400 300 250 200 150 400 350 1000;
ENDDATA
END