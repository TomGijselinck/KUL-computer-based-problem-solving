! Minimal cost network flow problem template;

MODEL:
SETS:
  NODES/1..6/:SUPP;
  ARCS(NODES,NODES)/1,2 1,3 2,5 2,4 3,5 3,4 4,5 4,6 5,6/:CAP,FLOW,COST;
ENDSETS

MIN=@SUM(ARCS:COST*FLOW);

@FOR(ARCS(I,J):FLOW(I,J)<CAP(I,J));
@FOR(NODES(I):-@SUM(ARCS(J,I):FLOW(J,I))+@SUM(ARCS(I,J):FLOW(I,J))=SUPP(I));

DATA:
  COST=10 50 70 30 30 30 60 60 10;

  SUPP=900,0,0,0,0,-900;
  CAP=800 600 600 100 300 400 400 600 600;
ENDDATA
END